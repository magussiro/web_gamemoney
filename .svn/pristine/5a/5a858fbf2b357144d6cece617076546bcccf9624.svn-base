/* #md_news_1 END */


/* #md_news_2 START */

#md_news_2 {
    padding: 20px;
}

#md #md_news_2 .md_rank_table {
    border-collapse: collapse;
    width: 100%;
    text-align: center;
    font-size: 12px;
    border: 1px solid #fff;
    background-color: rgba(135, 206, 250, 0.1);
}

#md #md_news_2 .md_rank_table th {
    border-bottom: 1px solid #fff;
}

#md #md_news_2 .md_rank_table th,
#md #md_news_2 .md_rank_table td {
    padding: 5px 0px;
    text-align: center;
}

#md #md_news_2 .md_rank_table td {
    color: rgb(173, 216, 230);
}


/* #md_news_2 END */
