#md_news_1 {
    padding: 20px;
}

#md #md_news_1 ul li {
    margin: 10px 0px;
}

#md #md_news_1 ul li span {
    margin-right: 5px;
}

#md #md_news_1 ul li span:last-child {
    color: rgb(173, 216, 230);
    cursor: pointer;
}

#md #md_news_1 ul .md_news_type_game {
    color: rgb(204, 0, 102);
}

#md #md_news_1 ul .md_news_type_acti {
    color: rgb(51, 153, 102);
}


