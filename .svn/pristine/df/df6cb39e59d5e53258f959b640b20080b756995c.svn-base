#wrap {
    /*max-width: 100%;
    min-width: 1300px;
    */
    width: 100%;
}

#wrap #header {
    -webkit-box-sizing: border-box;
    -moz-box-sizing: border-box;
    box-sizing: border-box;
    padding: 10px;
    background: -webkit-linear-gradient(top, rgba(233, 233, 232, 0.51) 0%, rgba(233, 233, 232, 0.51) 1%, #d4d4d3 100%);
    /* Chrome10-25,Safari5.1-6 */
    border: 1px solid #A8A8A8;
}

#wrap #header .header_left,
#wrap #header .header_middle,
#wrap #header .header_right {
    display: inline-block;
}

#wrap #header .header_left ul.mainmenu li.main,
#wrap #header .header_middle ul.mainmenu li.main,
#wrap #header .header_right ul.mainmenu li.main {
    display: inline-block;
    vertical-align: top;
    padding: 5px;
}

#wrap #header .header_left {
    width: 320px;
    margin-left: 10px;
}

#wrap #header .header_left ul.mainmenu {
    position: relative;
}

#wrap #header .header_left ul.mainmenu li.main {
    padding: 5px 10px;
    display: inline-block;
    cursor: pointer;
    background: -webkit-linear-gradient(top, rgba(192, 192, 192, 0.53) 0%, rgba(107, 106, 106, 0.92) 100%);
    /* Chrome10-25,Safari5.1-6 */
    color: #E8EBEC;
}

#wrap #header .header_left ul.mainmenu li.main ul.submenu {
    position: absolute;
    border: 2px solid #111;
    -webkit-border-radius: 5px;
    -moz-border-radius: 5px;
    border-radius: 5px;
    background: #E8E6E6;
    margin-left: -10px;
    margin-top: 5px;
    display: none;
}

#wrap #header .header_left ul.mainmenu li.main ul.submenu li.sub {
    display: block;
    line-height: 2;
    -webkit-box-sizing: border-box;
    -moz-box-sizing: border-box;
    box-sizing: border-box;
    padding: 5px;
}

#wrap #header .header_left ul.mainmenu li.main ul.submenu li.sub a {
    display: block;
    font-size: 12px;
    color: #111;
}

#wrap #header .header_left ul.mainmenu li.main ul.submenu li.sub a:hover {
    border: 1px solid #00f;
}

#wrap #header .header_left ul.mainmenu li.main ul.submenu li.sub:last-child {
    border-bottom: none;
}

#wrap #header .header_middle {
    width: 750px;
    color: #004489;
}

#wrap #header .header_right {
    width: 200px;
    text-align: right;
}

#wrap #header .header_right ul.mainmenu li.main {
    cursor: pointer;
}

#wrap #content #left_cont,
