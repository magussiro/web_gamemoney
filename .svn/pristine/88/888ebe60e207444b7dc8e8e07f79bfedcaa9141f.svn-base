/* #md_news_1 START */
$("#md_btn_all").click(function() {
    $(".md_news_type_game").parent().css("display", "block");
    $(".md_news_type_acti").parent().css("display", "block");
});
$("#md_btn_game").click(function() {
    $(".md_news_type_game").parent().css("display", "block");
    $(".md_news_type_acti").parent().css("display", "none");
});
$("#md_btn_acti").click(function() {
    $(".md_news_type_game").parent().css("display", "none");
    $(".md_news_type_acti").parent().css("display", "block");
});
/* #md_news_1 END */
